{
  "name": "digital-hometown-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn && yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "run-s deploy:*",
    "deploy:rules": "firebase deploy --only storage,firestore,database",
    "test": "react-scripts test --ci",
    "test:watch": "react-scripts test --watchAll",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier --write \"**/*.{ts,js,tsx,jsx,css,scss,html,json,md}\"",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier \"**/*.{ts,js,tsx,jsx,css,scss,html,json,md}\" --check",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@types/lodash": "^4.14.191",
    "cross-env": "^7.0.3",
    "firebase-tools": "^11.18.0"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.0",
    "@mui/x-data-grid": "^5.17.17",
    "@mui/x-date-pickers": "^5.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/axios": "^0.14.0",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^18.0.0",
    "axios": "^1.2.1",
    "axios-mock-adapter": "^1.20.0",
    "date-fns": "^2.28.0",
    "emoji-picker-react": "^4.4.4",
    "firebase": "^9.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.1",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-placeholder": "^4.1.0",
    "react-router-dom": "^6.4.3",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.6",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.1",
    "tsx": "^3.11.0",
    "typescript": "^4.9.4",
    "web-vitals": "^3.0.4"
  }
}
