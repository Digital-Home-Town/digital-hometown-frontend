{
  "name": "digital-hometown-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn && yarn start",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watchAll",
    "fix": "run-s fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier --write \"**/*.{ts,js,tsx,jsx,css,scss,html,json,md}\"",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint",
    "lint:prettier": "prettier \"**/*.{ts,js,tsx,jsx,css,scss,html,json,md}\" --check",
    "predeploy": "cross-env PUBLIC_URL=https://digital-home-town.github.io/digital-hometown-frontend && npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.8.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/axios-mock-adapter": "^1.10.0",
    "@types/jest": "^28.1.0",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "^17.0.1",
    "axios": "^0.25.0",
    "axios-mock-adapter": "^1.20.0",
    "firebase": "^9.6.10",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-placeholder": "^4.1.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.11.0",
    "react-test-renderer": "^17.0.2",
    "react-toastify": "^8.2.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  }
}
